<?xml version="1.0"?>
<block>
	<name>Function</name>
	<key>math</key>
	<category>Math Operators</category>
	<import>from gnuradio import iio</import>
	<make>iio.iio_math($function)</make>

	<param>
		<name>f(x) =</name>
		<key>function</key>
		<value></value>
		<type>string</type>
	</param>

	<!-- Refuse empty strings -->
	<check>$function</check>

	<sink>
		<name>in</name>
		<type>float</type>
	</sink>

	<source>
		<name>out</name>
		<type>float</type>
	</source>
</block>
