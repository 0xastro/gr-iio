<?xml version="1.0"?>
<block>
  <name>fftshift</name>
  <key>iio_fftshift</key>
  <category>[iio]</category>
  <import>import iio</import>
  <make>iio.fftshift($vlen)</make>

  <param>
    <name>vlen</name>
    <key>vlen</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>float</type>
    <vlen>$vlen</vlen>
  </sink>

  <source>
    <name>out</name>
    <type>float</type>
    <vlen>$vlen</vlen>
  </source>
</block>
